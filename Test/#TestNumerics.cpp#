#include "gtest/gtest
.h"
#include "Numerics.h"

using namespace std;

double realFunc(double x) {
  return 3*x*x-7*x;
}

 complexFunc

const double maxError = 1e-15;

TEST(IntegralTest, Real) {
  cout << Numerics::qgauss(realFunc, -0.2, 0.5) << endl;
  ASSERT_NEAR(Numerics::qgauss(realFunc, -0.2, 0.5), -0.602, maxError);
}
